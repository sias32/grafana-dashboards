            "uid": "${datasource}"
          },
          "editorMode": "code",
          "expr": "_stream:{host=$host, source_type=$source_type}\r\n| SYSLOG_IDENTIFIER:$service\r\n| log.level:($level)\r\n| _msg:~\"(?i)$query\"\r\n| stats by (log.level) count() c\r\n| sort by (c desc)\r\n| limit 10",
          "hide": false,
